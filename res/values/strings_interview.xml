<resources>
    
    <!-- interviewer_basic -->
    <string name="interview_interviewer_basic_title">请输入受访者个人信息，单击“下一步”按钮，进入DNA样本采集页面</string>
    
    <string name="interview_interviewer_basic_username">姓名：</string>
    <string name="interview_interviewer_basic_identity_card">身份证号码：</string>
    <string name="interview_interviewer_basic_province">省/自治区/直辖市：</string>
