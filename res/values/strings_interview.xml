    <string name="interview_interviewer_basic_remark">备注：</string>
    
    <string name="interview_interviewer_basic_username_hint">*至少2个中文字</string>
    <string name="interview_interviewer_basic_identity_card_hint">*18位数字或字母组合</string>
    <string name="interview_interviewer_basic_province_hint">*</string>
